<Window x:Class="StroyMat.MinCountWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:StroyMat"
        mc:Ignorable="d"
        Title="" Height="130" Width="250">
    <Grid>
        <StackPanel>
            <TextBlock>Введите новое минимальное значение</TextBlock>
            <TextBox Width="100" Margin="0,7"></TextBox>
            <Button Name="ButtOk" Width="50" Click="ButtOk_Click">OK</Button>
        </StackPanel>
    </Grid>
</Window>
